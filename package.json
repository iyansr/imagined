    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.87.4",
    "ai": "^5.0.41",
    "better-auth": "^1.3.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.543.0",
    "next": "15.4.6",
